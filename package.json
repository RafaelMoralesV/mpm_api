{
  "name": "mpm_api",
  "version": "1.0.0",
  "description": "API for the MPM flutter application",
  "main": "index.js",
  "scripts": {
    "test": "cross-env NODE_ENV=test jest --coverage --watchAll --verbose --detectOpenHandles",
    "test:silent": "cross-env NODE_ENV=test jest --coverage --watchAll --verbose --detectOpenHandles --silent",
    "dev": "cross-env NODE_ENV=development nodemon src/server.ts",
    "build": "cross-env NODE_ENV=production rm -rf build/ && prettier --write source/ && tsc",
    "migrate": "nodemon src/migrations/user_table.ts"
  },
  "keywords": [
    "API",
    "REST-API",
    "TypeScript"
  ],
  "author": "Rafael Morales Venegas",
  "license": "GPL-3.0-or-later",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.1",
    "dotenv": "^14.3.2",
    "express": "^4.17.2",
    "express-jwt": "^6.1.0",
    "jsonwebtoken": "^8.5.1",
    "mariadb": "^2.5.5",
    "nodemon": "^2.0.15",
    "sequelize": "^6.14.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.7",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@faker-js/faker": "^6.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/bluebird": "^3.5.36",
    "@types/express": "^4.17.13",
    "@types/express-jwt": "^6.0.4",
    "@types/jest": "^27.4.1",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^17.0.21",
    "@types/sequelize": "^4.28.11",
    "@types/supertest": "^2.0.11",
    "@types/validator": "^13.7.1",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "babel-jest": "^27.5.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.7.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.2.0",
    "jest": "^27.5.1",
    "sequelize-cli": "^6.4.1",
    "sqlite3": "^5.0.2",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.2"
  }
}
